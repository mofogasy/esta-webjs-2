{
  "name": "webjs-starterkit",
  "description": "This is a starterkit based on angular-cli with enterprise extensions",
  "version": "7.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o --aot",
    "lint": "ng lint",
    "test": "ng test --watch=false",
    "test-selenium": "ng test --progress=false --watch=false --browsers=SeleniumCH,SeleniumFF --code-coverage",
    "e2e-local": "ng e2e",
    "e2e-puppeteer": "ng e2e --protractor-config=e2e/protractor.puppeteer.conf.js",
    "pree2e-selenium": "cp e2e/protractor.webgrid.conf.js e2e/protractor.conf.js; sed -i \"s/\"PORT_VARIABLE\"/$(echo $externalport)/g\" angular.json; sed -i \"s/PUBLIC_HOST_VARIABLE/$(echo $host):$(echo $externalport)/g\" angular.json",
    "e2e-selenium": "ng e2e --dev-server-target=webjs-starterkit:serve-e2e-ci --host=0.0.0.0 --port=$(echo $externalport)",
    "build-prod-local": "ng build --prod",
    "build-prod-ci": "ng build --prod --progress=false"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.0.0",
    "@angular/common": "~7.0.0",
    "@angular/compiler": "~7.0.0",
    "@angular/core": "~7.0.0",
    "@angular/forms": "~7.0.0",
    "@angular/http": "~7.0.0",
    "@angular/platform-browser": "~7.0.0",
    "@angular/platform-browser-dynamic": "~7.0.0",
    "@angular/router": "~7.0.0",
    "core-js": "^2.5.4",
    "rxjs": "~6.3.3",
    "zone.js": "~0.8.26",
    "@ngx-translate/core": "^11.0.0",
    "@ngx-translate/http-loader": "^4.0.0",
    "esta-webjs-extensions": "^3.0.1",
    "font-awesome": "^4.7.0",
    "bootstrap": "^4.1.3",
    "primeng": "^6.1.6"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular/cli": "~7.0.5",
    "@angular/compiler-cli": "~7.0.0",
    "@angular/language-service": "~7.0.0",
    "@types/node": "~8.9.4",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6",
    "popper.js": "^1.14.5",
    "jquery": "^3.3.1",
    "@types/puppeteer": "^1.10.0",
    "jasmine-reporters": "^2.3.2",
    "karma-junit-reporter": "^1.2.0",
    "karma-webdriver-launcher": "^1.0.5",
    "my-local-ip": "^1.0.0",
    "puppeteer": "^1.10.0"
  },
  "keywords": [
    "esta-webjs-starterkit",
    "angularjs",
    "webpack"
  ],
  "author": {
    "name": "Reto Lehmann",
    "email": "reto.lehmann@sbb.ch"
  }
}
