{
  "name": "webjs-starterkit",
  "description": "This is a starterkit based on angular-cli with enterprise extensions",
  "version": "6.0.3",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "clean": "rimraf dist reports",
    "start": "ng serve -o --aot",
    "lint": "ng lint",
    "test": "ng test --watch=false",
    "test-selenium": "ng test --progress=false --watch=false --browsers=SeleniumCH,SeleniumFF --code-coverage",
    "e2e-local": "ng e2e",
    "pree2e-selenium": "cp e2e/protractor.webgrid.conf.js e2e/protractor.conf.js; sed -i \"s/\"PORT_VARIABLE\"/$(echo $externalport)/g\" angular.json; sed -i \"s/PUBLIC_HOST_VARIABLE/$(echo $host):$(echo $externalport)/g\" angular.json",
    "e2e-selenium": "ng e2e --dev-server-target=webjs-starterkit:serve-e2e-ci --host=0.0.0.0 --port=$(echo $externalport)",
    "build-prod-local": "ng build --prod",
    "build-prod-ci": "ng build --prod --progress=false"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "=6.0.3",
    "@angular/common": "=6.0.3",
    "@angular/compiler": "=6.0.3",
    "@angular/core": "=6.0.3",
    "@angular/forms": "=6.0.3",
    "@angular/http": "=6.0.3",
    "@angular/platform-browser": "=6.0.3",
    "@angular/platform-browser-dynamic": "=6.0.3",
    "@angular/router": "=6.0.3",
    "@ngx-translate/core": "=10.0.2",
    "@ngx-translate/http-loader": "=3.0.1",
    "bootstrap": "=4.1.1",
    "core-js": "=2.5.7",
    "esta-webjs-extensions": "=3.0.1",
    "font-awesome": "=4.7.0",
    "jquery": "=3.3.1",
    "node-sass": "^4.9.0",
    "popper.js": "=1.14.3",
    "primeng": "=6.0.0-alpha.1",
    "rxjs": "=6.2.0",
    "zone.js": "=0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "=0.6.5",
    "@angular/cli": "=6.0.5",
    "@angular/compiler-cli": "=6.0.3",
    "@angular/language-service": "=6.0.3",
    "@types/jasmine": "=2.8.7",
    "@types/jasminewd2": "=2.0.3",
    "@types/node": "=8.9.5",
    "@types/puppeteer": "^1.9.0",
    "codelyzer": "=4.2.1",
    "jasmine-core": "=2.99.1",
    "jasmine-reporters": "=2.2.1",
    "jasmine-spec-reporter": "=4.2.1",
    "karma": "=1.7.1",
    "karma-chrome-launcher": "=2.2.0",
    "karma-cli": "=1.0.1",
    "karma-coverage-istanbul-reporter": "=1.4.3",
    "karma-jasmine": "=1.1.2",
    "karma-jasmine-html-reporter": "=0.2.2",
    "karma-junit-reporter": "=1.2.0",
    "karma-nightmare": "=0.4.9",
    "karma-webdriver-launcher": "=1.0.5",
    "my-local-ip": "=1.0.0",
    "nightmare": "=3.0.1",
    "node-sass": "^4.9.0",
    "protractor": "=5.3.2",
    "puppeteer": "^1.9.0",
    "rimraf": "=2.6.2",
    "ts-node": "=5.0.1",
    "tslint": "=5.9.1",
    "typescript": "=2.7.2"
  },
  "keywords": [
    "esta-webjs-starterkit",
    "angularjs",
    "webpack"
  ],
  "author": {
    "name": "Reto Lehmann",
