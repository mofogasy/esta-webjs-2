{
  "name": "webjs-starterkit",
  "description": "This is a starterkit based on angular-cli with enterprise extensions",
  "version": "5.1.0-SNAPSHOT",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "clean": "rimraf dist reports",
    "start": "ng serve -o --aot",
    "lint": "ng lint --type-check",
    "test": "ng test --watch=false",
    "test-selenium": "ng test --progress=false --watch=false --browsers=SeleniumCH,SeleniumFF --code-coverage",
    "e2e": "ng e2e",
    "e2e-selenium": "ng e2e --progress=false --host=0.0.0.0 --disableHostCheck=true --port=$(echo $externalport) --public-host=$(echo $host):$(echo $externalport) --config=protractor.webgrid.conf.js",
    "analyze-bundle": "ng build --prod --stats-json && webpack-bundle-analyzer dist/stats.json",
    "build-prod-local": "ng build --prod",
    "build-prod-ci": "ng build --prod --progress=false"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "=5.1.0",
    "@angular/common": "=5.1.0",
    "@angular/compiler": "=5.1.0",
    "@angular/core": "=5.1.0",
    "@angular/forms": "=5.1.0",
    "@angular/http": "=5.1.0",
    "@angular/platform-browser": "=5.1.0",
    "@angular/platform-browser-dynamic": "=5.1.0",
    "@angular/router": "=5.1.0",
    "@ngx-translate/core": "=9.0.1",
    "@ngx-translate/http-loader": "=2.0.0",
    "bootstrap": "=4.0.0-beta.2",
    "core-js": "=2.5.1",
    "esta-webjs-extensions": "=2.0.0",
    "font-awesome": "=4.7.0",
    "jquery": "=3.2.1",
    "popper.js": "=1.12.5",
    "primeng": "=4.2.2",
    "rxjs": "=5.5.2",
    "zone.js": "=0.8.17"
  },
  "devDependencies": {
    "@angular/cli": "1.6.0",
    "@angular/compiler-cli": "=5.1.0",
    "@types/jasmine": "=2.5.54",
    "@types/jasminewd2": "=2.0.2",
    "@types/node": "=8.0.20",
    "codelyzer": "=4.0.1",
    "jasmine-core": "=2.7.0",
    "jasmine-reporters": "^2.2.1",
    "jasmine-spec-reporter": "=4.2.0",
    "karma": "=1.7.0",
    "karma-chrome-launcher": "=2.2.0",
    "karma-cli": "=1.0.1",
    "karma-coverage-istanbul-reporter": "=1.3.0",
    "karma-jasmine": "=1.1.0",
    "karma-jasmine-html-reporter": "=0.2.2",
    "karma-junit-reporter": "=1.2.0",
    "karma-nightmare": "=0.4.9",
    "karma-webdriver-launcher": "=1.0.5",
    "my-local-ip": "=1.0.0",
    "nightmare": "=2.10.0",
    "protractor": "=5.1.2",
    "rimraf": "^2.6.2",
    "ts-node": "=3.2.2",
    "tslint": "=5.7.0",
    "typescript": "=2.4.2",
    "webpack-bundle-analyzer": "^2.9.1"
  },
  "keywords": [
    "esta-webjs-starterkit",
    "angularjs",
    "webpack"
  ],
  "author": {
    "name": "Reto Lehmann",
    "email": "reto.lehmann@sbb.ch"
  }
}
