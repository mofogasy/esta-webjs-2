{
  "name": "webjs-starterkit",
  "description": "This is a starterkit based on angular-cli with enterprise extensions",
  "version": "4.3.4",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "lint": "ng lint --type-check",
    "test": "ng test --watch=false",
    "test-selenium": "ng test --progress=false --watch=false --browsers=SeleniumCH,SeleniumFF --code-coverage",
    "e2e": "ng e2e",
    "e2e-selenium": "ng e2e --progress=false --host=0.0.0.0 --disableHostCheck=true --port=$(echo $externalport) --public-host=$(echo $host):$(echo $externalport) --config=protractor.webgrid.conf.js",
    "prebuild-prod": "npm run test-selenium && npm run e2e-selenium",
    "build-prod": "ng build --prod --progress=false"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "=4.3.4",
    "@angular/common": "=4.3.4",
    "@angular/compiler": "=4.3.4",
    "@angular/core": "=4.3.4",
    "@angular/forms": "=4.3.4",
    "@angular/http": "=4.3.4",
    "@angular/platform-browser": "=4.3.4",
    "@angular/platform-browser-dynamic": "=4.3.4",
    "@angular/router": "=4.3.4",
    "@ngx-translate/core": "=7.1.0",
    "@ngx-translate/http-loader": "=1.0.2",
    "bootstrap": "=4.0.0-alpha.6",
    "core-js": "=2.5.0",
    "esta-webjs-extensions": "=1.1.3",
    "font-awesome": "=4.7.0",
    "primeng": "=4.1.3",
    "rxjs": "=5.4.2",
    "zone.js": "=0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.3.0",
    "@angular/compiler-cli": "=4.3.4",
    "@types/jasmine": "=2.5.53",
    "@types/node": "=8.0.20",
    "codelyzer": "=3.1.2",
    "jasmine-core": "=2.7.0",
    "jasmine-spec-reporter": "=4.2.0",
    "karma": "=1.7.0",
    "karma-chrome-launcher": "=2.2.0",
    "karma-cli": "=1.0.1",
    "karma-coverage-istanbul-reporter": "=1.3.0",
    "karma-jasmine": "=1.1.0",
    "karma-jasmine-html-reporter": "=0.2.2",
    "karma-nightmare": "=0.4.9",
    "karma-webdriver-launcher": "=1.0.5",
    "my-local-ip": "=1.0.0",
    "nightmare": "=2.10.0",
    "protractor": "=5.1.2",
    "ts-node": "=3.0.6",
    "tslint": "=5.4.3",
    "typescript": "=2.3.3"
  },
  "keywords": [
    "esta-webjs-starterkit",
    "angularjs",
    "webpack"
  ],
  "author": {
    "name": "Reto Lehmann",
    "email": "reto.lehmann@sbb.ch"
  }
}
